{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist", "out", "build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "docker:web:start": {
      "dependsOn": ["^docker:web:start"]
    },
    "docker:scripts:db:sync": {
      "dependsOn": ["^docker:scripts:db:sync"],
      "cache": false
    }
  }
}
